<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Path Selector Design Dialog"
          sling:resourceType="cq/gui/components/authoring/designdialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <designTab jcr:primaryType="nt:unstructured"
                               jcr:title="Design"
                               sling:resourceType="granite/ui/components/coral/foundation/tabpanel">
                        <items jcr:primaryType="nt:unstructured">
                            <pathselector jcr:primaryType="nt:unstructured"
                                          sling:resourceType="cq/gui/components/authoring/dialog/pathfield"
                                          fieldLabel="Default Path"
                                          name="./defaultPath"
                                          rootPath="/content"/>
                        </items>
                    </designTab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
